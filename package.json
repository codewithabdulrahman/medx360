{
  "name": "medx360-plugin",
  "version": "1.0.0",
  "description": "MedX360 - Healthcare appointment booking system built with React",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev:admin": "vite --mode admin",
    "dev:frontend": "vite --mode frontend",
    "dev:all": "concurrently \"npm run dev:admin\" \"npm run dev:frontend\"",
    "build": "vite build",
    "build:admin": "vite build --mode admin",
    "build:frontend": "vite build --mode frontend",
    "build:all": "npm run build:admin && npm run build:frontend",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fullcalendar/daygrid": "^6.0.0",
    "@fullcalendar/interaction": "^6.0.0",
    "@fullcalendar/react": "^6.0.0",
    "@fullcalendar/timegrid": "^6.0.0",
    "@hookform/resolvers": "^2.9.11",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.0",
    "@mui/x-data-grid": "^5.17.0",
    "@mui/x-date-pickers": "^5.0.20",
    "@reduxjs/toolkit": "^1.9.0",
    "axios": "^1.3.0",
    "dayjs": "^1.11.7",
    "framer-motion": "^10.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.0",
    "react-hook-form": "^7.43.0",
    "react-hook-form-mui": "^5.0.0",
    "react-hot-toast": "^2.4.0",
    "react-intersection-observer": "^9.4.0",
    "react-query": "^3.39.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.0",
    "react-virtualized": "^9.22.0",
    "recharts": "^2.5.0",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.0",
    "@types/node": "^18.15.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^3.1.0",
    "concurrently": "^9.2.1",
    "eslint": "^8.35.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.4.0",
    "jest-environment-jsdom": "^29.4.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^1.7.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
